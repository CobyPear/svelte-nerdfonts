{
  "name": "svelte-nerdfonts",
  "version": "1.0.0",
  "author": "icalvin102",
  "license": "MIT",
  "description": "Use all nerdfont-icons in your Svelte project",
  "main": "components/index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf icons",
    "generate": "python nerdconvert.py -o es icons --fields iconname viewbox paths",
    "build": "rollup -c demo/rollup.config.js",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "keywords": [
    "nerdfonts",
    "icons",
    "svelte"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
